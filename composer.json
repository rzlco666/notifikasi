{
    "name": "rzlco/notifikasi",
    "description": "A beautiful Apple-style liquid glass notification system for Laravel with real-time updates and modern UI",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "notification", "toast", "ui", "glass", "apple", "liquid"],
    "authors": [
        {
            "name": "Rizki",
            "email": "rizki@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rzlco\\Notifikasi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rzlco\\Notifikasi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rzlco\\Notifikasi\\NotifikasiServiceProvider"
            ],
            "aliases": {
                "Notifikasi": "Rzlco\\Notifikasi\\Facades\\Notifikasi"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "0.1.2"
} 